# Копировать в .env в apps/server/ и заполнить реальными значениями

# PostgreSQL (если используешь docker-compose — оставь как есть)
DATABASE_URL=postgresql://postgres:postgres@localhost:5432/citydb?schema=public

# Секретный ключ для JWT (обязательно смени на свой длинный случайный)
JWT_SECRET=your_super_long_random_secret_key_here_change_this_1234567890

# Порт бэкенда (можно оставить 5000)
PORT=5000